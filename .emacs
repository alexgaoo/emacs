(add-to-list 'load-path "~/.emacs.d/themes/")

(add-to-list 'custom-theme-load-path "~/.emacs.d/themes")

(add-to-list 'load-path "/some/path/neotree")
(require 'neotree)
(global-set-key [f8] 'neotree-toggle)

(global-set-key [f4] 'linum-mode)

